    "pop_0010_cleaned[\"STNAME\"] = pop_0010_cleaned[\"STNAME\"].str.upper()\n",
    "pop_0010_cleaned[\"CTYNAME\"] = pop_0010_cleaned[\"CTYNAME\"].str.upper()\n",
    "\n",
    "pop_0010_cleaned.columns = (\n",
